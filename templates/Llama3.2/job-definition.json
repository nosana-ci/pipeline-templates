          ],
          "gpu": true,
          "image": "docker.io/vllm/vllm-openai:v0.7.2",
          "expose": 9000,
          "entrypoint": []
        },
        "type": "container/run"
      }
    ],
    "meta": {
      "trigger": "dashboard",
      "system_requirements": {
        "required_vram": 28
      }
    },
    "type": "container",
    "version": "0.1"
  }
  