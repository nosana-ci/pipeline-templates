{
    "version": "0.1",
    "type": "container",
    "meta": {
      "trigger": "dashboard",
      "system_requirements": {
        "required_vram": 5
      }
    },
    "ops": [
      {
        "type": "container/run",
        "id": "whisper",
        "args": {
          "image": "docker.io/onerahmet/openai-whisper-asr-webservice:v1.8.2-gpu",
          "cmd": [],
          "gpu": true,
          "expose": 9000,
          "env": {
            "ASR_MODEL": "large",
            "ASR_ENGINE": "openai_whisper"
          }
        }
      }
    ]
  }
  