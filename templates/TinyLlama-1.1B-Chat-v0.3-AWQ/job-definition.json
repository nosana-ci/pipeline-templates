{
    "ops": [
      {
        "id": "tinyllama",
        "args": {
          "cmd": [
            "/bin/sh",
            "-c",
            " python3 -m vllm.entrypoints.api_server --model TheBloke/TinyLlama-1.1B-Chat-v0.3-AWQ --quantization awq --dtype half --port 9000"
          ],
          "gpu": true,
          "image": "docker.io/vllm/vllm-openai:v0.7.2",
          "expose": 9000,
          "entrypoint": []
        },
        "type": "container/run"
      }
    ],
    "meta": {
      "trigger": "dashboard",
      "system_requirements": {
        "required_vram": 6
      }
    },
    "type": "container",
    "version": "0.1"
  }
  