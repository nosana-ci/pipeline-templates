{
  "ops": [
    {
      "id": "gem12b",
      "args": {
        "cmd": [
          "/bin/sh",
          "-c",
          "python3 -m vllm.entrypoints.openai.api_server --model google/gemma-3-12b-it --served-model-name Gemma-3-12b --port 9000 --max-model-len 10000"
        ],
        "env": {
          "HF_TOKEN": "fill_in_your_huggingface_token"
        },
        "gpu": true,
        "image": "nosana/vllm:0.0.0",
        "expose": 9000,
        "entrypoint": []
      },
      "type": "container/run"
    }
  ],
  "meta": {
    "trigger": "dashboard",
    "system_requirements": {
      "required_vram": 60
    }
  },
  "type": "container",
  "version": "0.1"
}