{
  "version": "0.1",
  "type": "container",
  "meta": {
    "trigger": "dashboard",
    "system_requirements": {
      "required_vram": 4
    }
  },
  "ops": [
    {
      "type": "container/run",
      "id": "axo",
      "args": {
        "image": "docker.io/winglian/axolotl-cloud:main-20250221",
        "cmd": [
          "jupyter",
          "notebook",
          "--ip=0.0.0.0",
          "--port=8888",
          "--no-browser",
          "--allow-root",
          "--ServerApp.token=''",
          "--ServerApp.password=''"
        ],
        "expose": 8888,
        "gpu": true
      }
    }
  ]
}
