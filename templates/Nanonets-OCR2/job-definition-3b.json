{
  "version": "0.1",
  "type": "container",
  "global": {
    "variables": {
      "MODEL": "nanonets/Nanonets-OCR2-3B"
    }
  },
  "ops": [
    {
      "id": "Nanonets-OCR2:3B",
      "type": "container/run",
      "args": {
        "cmd": [
          "--model",
          "%%global.variables.MODEL%%",
          "--served-model-name",
          "%%global.variables.MODEL%%",
          "--port",
          "8000",
          "--max-model-len",
          "30000"
        ],
        "gpu": true,
        "image": "docker.io/vllm/vllm-openai:v0.10.2",
        "expose": 8000
      }
    }
  ],
  "meta": {
    "trigger": "dashboard",
    "system_requirements": {
      "required_vram": 8
    }
  }
}